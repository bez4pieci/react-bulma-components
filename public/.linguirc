{
   "localeDir": "src/locales/",
   "srcPathDirs": ["src/"],
   "format": "po",
   "fallbackLocale": "en",
   # "sourceLocale": "en",
   extractBabelOptions: {
      "presets": ["react-app"],
      "plugins": [
         "macros",
         "@babel/plugin-transform-async-to-generator",
         "@babel/plugin-syntax-dynamic-import",
         "@babel/plugin-proposal-object-rest-spread",
         "@babel/plugin-proposal-class-properties"
      ],
   }
}